<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="velikonja_labby.service.syncer.class">Velikonja\LabbyBundle\Service\Syncer</parameter>
        <parameter key="velikonja_labby.service.dumper.class">Velikonja\LabbyBundle\Database\MySqlDumper</parameter>
    </parameters>

    <services>
        <service id="velikonja_labby.service.syncer" class="%velikonja_labby.service.syncer.class%">
            <argument>%velikonja_labby.config%</argument>
        </service>
        <service id="velikonja_labby.service.dumper" class="%velikonja_labby.service.dumper.class%">
            <argument>%velikonja_labby.config.db%</argument>
        </service>
    </services>
</container>
